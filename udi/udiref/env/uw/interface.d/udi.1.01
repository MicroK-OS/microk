$entrytype 1
$depend udi

# Internal symbols shared between UDI modules, not meant to be
# used by drivers.
_udi_module_load
_udi_module_unload

# Management Metalanguage
udi_usage_res
udi_enumerate_ack
udi_devmgmt_ack
udi_final_cleanup_ack
udi_static_usage
udi_enumerate_no_children

# Control block management

udi_cb_alloc
udi_cb_alloc_batch
udi_cb_alloc_dynamic
udi_cb_free
udi_cancel

# Memory management

udi_mem_alloc
udi_mem_free

# Buffer management

udi_buf_copy
udi_buf_write
udi_buf_read
udi_buf_free
udi_buf_best_path
udi_buf_path_alloc
udi_buf_path_free
udi_buf_tag_get
udi_buf_tag_set
udi_buf_tag_compute
udi_buf_tag_apply

# time services

udi_timer_start
udi_timer_cancel
udi_time_current
udi_time_between
udi_time_since
udi_timer_start_repeating

# Instance attributes

udi_instance_attr_get
udi_instance_attr_set

# Inter-module communication

udi_channel_anchor
udi_channel_spawn
udi_channel_set_context
udi_channel_op_abort
udi_channel_close
udi_channel_event_complete

# Tracing and logging.

udi_trace_write
udi_log_write
udi_debug_printf

# Debugging 

udi_assert
udi_debug_break

# Utilities: Output Formatting
udi_snprintf
udi_vsnprintf

# Utilities: String Manipulation
udi_strcat	strcat
udi_strncat	strncat
udi_strcmp	strcmp
udi_strncmp	strncmp
udi_memcmp	bcmp
udi_strcpy	strcpy
udi_strncpy	strncpy
udi_memcpy
udi_memmove
udi_strchr	strchr
udi_strrchr
udi_memchr
udi_strtou32	strtoul
udi_memset
udi_strlen	strlen
udi_strncpy_rtrim

# Utilities: Queue Management
udi_enqueue
udi_dequeue

# Utilities: Endian Management

udi_endian_swap

# Utilities: Assertion Support
__udi_assert
